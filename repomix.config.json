{
  "$schema": "https://repomix.com/schemas/latest/schema.json",
  "input": {
    "maxFileSize": 52428800
  },
  "output": {
    "filePath": "./public/llms-context/scalekit-node-sdk.txt",
    "style": "plain",
    "parsableStyle": false,
    "fileSummary": true,
    "topFilesLength": 0,
    "directoryStructure": false,
    "files": true,
    "useGitignore": true,
    "removeComments": false,
    "truncateBase64": true,
    "removeEmptyLines": false,
    "compress": true,
    "showLineNumbers": false,
    "copyToClipboard": false,
    "git": {
      "sortByChanges": true,
      "sortByChangesMaxCommits": 100,
      "includeDiffs": false
    }
  },
  "include": ["src/content/docs/**/*"],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": []
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}
